<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<DialogPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.orhanararat.Controller.Delete">

    <content>
        <GridPane hgap="10" vgap="10">
            <Label text="Ilan Silme Islemi" GridPane.columnIndex="0" GridPane.rowIndex="0">
            <font>
               <Font name="Times New Roman Bold" size="15.0" />
            </font></Label>
            <Label text="Ilani Seciniz " GridPane.columnIndex="0" GridPane.rowIndex="1" />
            <ComboBox fx:id="cbIlanSec" promptText="Ilani Seciniz." GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button fx:id="btnIlanSil" onAction="#ilanSil" text="Sil" GridPane.columnIndex="1" GridPane.rowIndex="2" />

            <Label text="Araba Silme Islemi" GridPane.columnIndex="2" GridPane.rowIndex="0">
            <font>
               <Font name="Times New Roman Bold" size="15.0" />
            </font></Label>
            <Label text="Arabayi Seciniz " GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <ComboBox fx:id="cbArabaSec" promptText="Araba Seciniz." GridPane.columnIndex="3" GridPane.rowIndex="1" />
            <Button fx:id="btnArabaSil" onAction="#arabaSil" text="Sil" GridPane.columnIndex="3" GridPane.rowIndex="2" />

            <Label text="Yakit Silme Islemi" GridPane.columnIndex="0" GridPane.rowIndex="3">
            <font>
               <Font name="Times New Roman Bold" size="15.0" />
            </font></Label>
            <Label text="Yakit Turunu Seciniz " GridPane.columnIndex="0" GridPane.rowIndex="4" />
            <ComboBox fx:id="cbYakitSec" promptText="Yakit Seciniz." GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Button fx:id="tbnYakitSil" onAction="#yakitSil" text="Sil" GridPane.columnIndex="1" GridPane.rowIndex="5" />

            <Label text="Vites Silme Islemi" GridPane.columnIndex="2" GridPane.rowIndex="3">
            <font>
               <Font name="Times New Roman Bold" size="15.0" />
            </font></Label>
            <Label text="Vites Turunu Seciniz " GridPane.columnIndex="2" GridPane.rowIndex="4" />
            <ComboBox fx:id="cbVitesSec" promptText="Vites Seciniz." GridPane.columnIndex="3" GridPane.rowIndex="4" />
            <Button fx:id="btnVitesSil" onAction="#vitesSil" text="Sil" GridPane.columnIndex="3" GridPane.rowIndex="5" />

            <Label text="Renk Silme Islemi" GridPane.columnIndex="0" GridPane.rowIndex="6">
            <font>
               <Font name="Times New Roman Bold" size="15.0" />
            </font></Label>
            <Label text="Rengi Seciniz " GridPane.columnIndex="0" GridPane.rowIndex="7" />
            <ComboBox fx:id="cbRenkSec" promptText="Renk Seciniz." GridPane.columnIndex="1" GridPane.rowIndex="7" />
            <Button fx:id="btnRenkSil" onAction="#renkSil" text="Sil" GridPane.columnIndex="1" GridPane.rowIndex="8" />

            <Label text="Sehir Silme Islemi" GridPane.columnIndex="2" GridPane.rowIndex="6">
            <font>
               <Font name="Times New Roman Bold" size="15.0" />
            </font></Label>
            <Label text="Sehiri Seciniz " GridPane.columnIndex="2" GridPane.rowIndex="7" />
            <ComboBox fx:id="cbSehirSec" promptText="Sehir Seciniz." GridPane.columnIndex="3" GridPane.rowIndex="7" />
            <Button fx:id="btnSehirSil" onAction="#sehirSil" text="Sil" GridPane.columnIndex="3" GridPane.rowIndex="8" />
         <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints />
            <ColumnConstraints />
            <ColumnConstraints />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
         </rowConstraints>
        </GridPane>
    </content>
</DialogPane>
